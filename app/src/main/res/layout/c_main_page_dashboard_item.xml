<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/dashboard_ll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_weight="0"
    android:background="@color/transparent"
    >
    <android.support.v7.widget.CardView
        android:id="@+id/dashboard_card_view"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        card_view:cardCornerRadius="4dp"
        android:clickable="true"
        android:focusable="true"
        android:focusableInTouchMode="true"
        card_view:cardElevation="0dp"
        android:background="@color/white"
        >
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="10dp"
            >

            <RelativeLayout
                android:id="@+id/dashboard_school_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                >
                <TextView
                    android:id="@+id/dashboard_school"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:textSize="13sp"
                    android:textColor="@color/wc_blue"
                    android:layout_alignParentLeft="true"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/dashboard_person_time_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                >
                <TextView
                    android:id="@+id/dashboard_person"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:textSize="12sp"
                    android:textColor="@color/text_light"
                    android:layout_alignParentLeft="true"
                    />
                <TextView
                    android:id="@+id/dashboard_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:textSize="12sp"
                    android:textColor="@color/text_very_light"
                    android:layout_alignParentRight="true"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/dashboard_title_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                >
                <TextView
                    android:id="@+id/dashboard_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:textSize="14sp"
                    android:textColor="@color/text_normal"
                    android:layout_alignParentLeft="true"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/dashboard_photo_rl"
                android:layout_weight="0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >
                <ImageView
                    android:id="@+id/dashboard_photo"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/dashboard_assess"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginBottom="7dp"
                >
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0"
                    >
                    <RelativeLayout
                        android:layout_weight="0"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="어려움: "
                            android:textSize="13sp"
                            android:textColor="@color/text_light"
                            android:layout_centerVertical="true"
                            />
                    </RelativeLayout>
                    <ImageView
                        android:id="@+id/assess_difficulty_1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_difficulty_2"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_difficulty_3"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_difficulty_4"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_difficulty_5"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0"
                    >
                    <RelativeLayout
                        android:layout_weight="0"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="유익함: "
                            android:textSize="13sp"
                            android:textColor="@color/text_light"
                            android:layout_centerVertical="true"
                            />
                    </RelativeLayout>
                    <ImageView
                        android:id="@+id/assess_instructiveness_1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_instructiveness_2"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_instructiveness_3"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_instructiveness_4"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                    <ImageView
                        android:id="@+id/assess_instructiveness_5"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/assess_star_empty"
                        />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/dashboard_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:textColor="@color/text_light"
                android:textSize="13sp"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginBottom="20dp"
                />

            <RelativeLayout
                android:id="@+id/dashboard_url_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="20dp"
                >
                <TextView
                    android:id="@+id/dashboard_url"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/transparent"
                    android:textSize="14sp"
                    android:textColor="@color/wc_dark_blue"
                    android:layout_centerInParent="true"
                    />

            </RelativeLayout>


        </LinearLayout>
    </android.support.v7.widget.CardView>

    <include layout="@layout/a_common_downloading_empty_last"/>

</LinearLayout>
